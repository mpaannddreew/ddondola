{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "aschmelyun/larametrics": "^1.1",
        "beyondcode/laravel-websockets": "^1.0",
        "fannypack/laracombee-multi-db": "1.0.1",
        "fannypack/momo": "^1.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "laravolt/avatar": "^2.2",
        "mll-lab/laravel-graphql-playground": "^1.0",
        "nuwave/lighthouse": "^2.6",
        "overtrue/laravel-follow": "^1.1",
        "pusher/pusher-php-server": "~3.0",
        "rinvex/countries": "^5.0",
        "silber/bouncer": "v1.0.0-rc.3",
        "spatie/laravel-medialibrary": "^7.0.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Ddondola\\": "app/",
            "Shoppie\\": "extensions/ddondola/shoppie/src",
            "Messenger\\": "extensions/ddondola/messenger/src",
            "Teller\\": "extensions/ddondola/teller/src",
            "Activity\\": "extensions/ddondola/activity/src",
            "Bank\\": "extensions/ddondola/bank/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "amranidev/laracombee"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
